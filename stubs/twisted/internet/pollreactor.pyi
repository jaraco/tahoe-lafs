from twisted.internet import posixbase
from typing import Any

class PollReactor(posixbase.PosixReactorBase, posixbase._PollLikeMixin):
    def __init__(self) -> None: ...
    def addReader(self, reader: Any) -> None: ...
    def addWriter(self, writer: Any) -> None: ...
    def removeReader(self, reader: Any): ...
    def removeWriter(self, writer: Any): ...
    def removeAll(self): ...
    def doPoll(self, timeout: Any) -> None: ...
    doIteration: Any = ...
    def getReaders(self): ...
    def getWriters(self): ...

def install() -> None: ...
